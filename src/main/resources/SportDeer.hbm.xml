<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zygimantus.sportdeerclient.SportDeerDocCountries" table="SD_COUNTRIES">
        <id column="ID" name="id" type="integer"/>
        <property column="NAME" name="name" type="string"/>
        <property column="SQ_FLAG" name="squareFlag" type="string"/>
        <property column="RC_FLAG" name="rectangleFlag" type="string"/>
    </class>
    <class name="com.zygimantus.sportdeerclient.SportDeerDocLeagues" table="SD_LEAGUES">
        <id column="ID" name="id" type="integer"/>
        <property column="ID_COUNTRY" name="idCountry" type="integer"/>
        <property column="NAME" name="name" type="string"/>
        <property column="LOGO" name="logoSvg" type="string"/>
    </class>
</hibernate-mapping>
