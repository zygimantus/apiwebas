<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.zygimantus.apiwebas.model.Apiwebas" optimistic-lock="version" table="APIWEBAS">
    <id column="ID" name="id" type="integer"/>
    <property column="TITLE" name="title" type="string"/>
    <property column="RESOURCE" name="resource" type="string"/>
  </class>
  <query name="findApiwebasByCategory">
FROM Apiwebas WHERE resource = :resource  
    </query>
</hibernate-mapping>
