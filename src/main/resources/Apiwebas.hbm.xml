<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zygimantus.apiwebas.model.Apiwebas" table="APIWEBAS" optimistic-lock="version">
        <id name="id" column="ID" type="integer"></id>
        <property name="title" column="TITLE" type="string"></property>
        <property name="category" column="CATEGORY" type="string"></property>        
    </class>
    <query name="findApiwebasByCategory">
<![CDATA[FROM Apiwebas WHERE category = :category]]>  
    </query>  
</hibernate-mapping>
