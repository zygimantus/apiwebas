{
	"$id": "http://example.com/example.json",
	"type": "object",
	"definitions": {
		
	},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"docs": {
			"$id": "/properties/docs",
			"type": "array",
			"items": {
				"$id": "/properties/docs/items",
				"type": "object",
				"properties": {
					"_id": {
						"$id": "/properties/docs/items/properties/_id",
						"type": "integer",
						"title": "The _id Schema ",
						"default": 0,
						"examples": [
							25
						]
					},
					"name": {
						"$id": "/properties/docs/items/properties/name",
						"type": "string",
						"title": "The Name Schema ",
						"default": "",
						"examples": [
							"Belgium"
						]
					},
					"rectangle_flag": {
						"$id": "/properties/docs/items/properties/rectangle_flag",
						"type": "string",
						"title": "The Rectangle_flag Schema ",
						"default": "",
						"examples": [
							"http://assets.sportdeer.com/flags/svg/4x3/be.svg"
						]
					},
					"square_flag": {
						"$id": "/properties/docs/items/properties/square_flag",
						"type": "string",
						"title": "The Square_flag Schema ",
						"default": "",
						"examples": [
							"http://assets.sportdeer.com/flags/svg/1x1/be.svg"
						]
					}
				}
			}
		},
		"pagination": {
			"$id": "/properties/pagination",
			"type": "object",
			"properties": {
				"total": {
					"$id": "/properties/pagination/properties/total",
					"type": "integer",
					"title": "The Total Schema ",
					"default": 0,
					"examples": [
						8
					]
				},
				"limit": {
					"$id": "/properties/pagination/properties/limit",
					"type": "integer",
					"title": "The Limit Schema ",
					"default": 0,
					"examples": [
						30
					]
				},
				"page": {
					"$id": "/properties/pagination/properties/page",
					"type": "integer",
					"title": "The Page Schema ",
					"default": 0,
					"examples": [
						1
					]
				},
				"pages": {
					"$id": "/properties/pagination/properties/pages",
					"type": "integer",
					"title": "The Pages Schema ",
					"default": 0,
					"examples": [
						1
					]
				}
			}
		}
	}
}

